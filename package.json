    "mocha": "^2.5.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
