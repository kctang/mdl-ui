    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-react-require": "^2.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "babel-root-slash-import": "^1.1.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
